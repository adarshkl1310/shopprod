<%=form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
   <b> <%= f.label :email,'Email' %> : </b><br />

    <%= f.email_field :email, autofocus: true,:size=>"50",:maxlength=>"55",class: "form-control"   %>
  </div>
</br>
  <div class="field">
    <b><%= f.label :password %> : </b><br />
    <%= f.password_field :password, autocomplete: "off",:size=>"50",:maxlength=>"55",class: "form-control"    %>
  </div></br>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%></br>

  <div class="actions">
    <%= f.submit "Log in",class: "my-primary btn" %>
  </div>
<% end %>
